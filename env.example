AUTH_GOOGLE_ID=your-google-client-id
AUTH_GOOGLE_SECRET=your-google-client-secret
AUTH_SECRET=generate-a-random-secret-here
# Generate a secret with: npx auth secret

# Database Configuration
# For local development, use POSTGRES_URL_DEV to point to a separate dev database
# This prevents test data from appearing in production
POSTGRES_URL_DEV=your_dev_database_connection_string
# POSTGRES_URL is used by @vercel/postgres in production
# Set it in Vercel Dashboard â†’ Environment Variables for Production
POSTGRES_URL=your_production_database_connection_string
